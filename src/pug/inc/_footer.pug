//- include global variables
include _var
// Start footer
footer(dir="ltr")
	.row
		.col
			| copyright &copy; 2018 - <span id="thisYear"></span> , created by #{author} , <b>MAF</b> &trade;
		.col
			each linkIcon, linkHref in socialLinks
				a(href=linkHref, class="circle m-1 bg-light")
					i(class="fa-fw fa-" + linkIcon)

// End footer
// include script files
each file in jsFiles
	script(src=jsDir + file)